# The script to invoke the QDL interpreter.
QDL_HOME=.
JAR_LIB="$QDL_HOME/lib/cp"
CFG_FILE="$QDL_HOME/etc/min-cfg.xml"
CFG_NAME="default"
QDL_JAR="$QDL_HOME/lib/qdl.jar"

cfgFile=${1:-$CFG_FILE}
cfgName=${2:-$CFG_NAME}

java -cp "$JAR_LIB/*" -jar $QDL_JAR  -cfg  $cfgFile -name $cfgName -home_dir $QDL_HOME -run "$@"