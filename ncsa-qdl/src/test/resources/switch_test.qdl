*/
while[
  for_next(j, 5)
 ]do[
    switch[
        if[j == 0]then[say('zero');];
        if[j == 1]then[say('   one');];
        if[j == 2]then[say(' two');];
        if[j == 3]then[say(' three');];
    ]; // end switch
]; // end do

/*
  The expected ouput:

  zero
     one
   two
   three
*/